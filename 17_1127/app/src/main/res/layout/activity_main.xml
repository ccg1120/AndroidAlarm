<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ccg11.testapplication17_1127.MainActivity">

    <EditText
        android:id="@+id/Alarm_Title"
        android:layout_width="379dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/But_hour_Up"
        android:layout_marginStart="32dp"
        android:layout_marginTop="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/TimeView_Hour"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="11"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="76dp" />

    <EditText
        android:id="@+id/timeView_Minute"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="41"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="76dp" />

    <Button
        android:id="@+id/But_hour_Up"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Up"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="151dp"
        android:onClick="HourUpCount"/>

    <Button
        android:id="@+id/But_hour_Down"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Down"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="216dp"
        android:onClick="HourDownCount"/>

    <Button
        android:id="@+id/But_Minute_Up"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Up"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="151dp"
        android:onClick="MinUpCount"
        />

    <Button
        android:id="@+id/But_Minute_Down"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Down"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="216dp"
        android:onClick="MinDownCount"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="59dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button6"
        android:layout_alignBottom="@+id/button6"
        android:layout_alignParentStart="true"
        android:text="월"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="279dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="59dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button9"
        android:layout_marginBottom="14dp"
        android:layout_toEndOf="@+id/button5"
        android:text="화"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="279dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="59dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button6"
        android:layout_alignBottom="@+id/button6"
        android:layout_toEndOf="@+id/button6"
        android:text="수"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="279dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="59dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button7"
        android:layout_alignBottom="@+id/button7"
        android:layout_toEndOf="@+id/button7"
        android:text="목"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="279dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="59dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="130dp"
        android:layout_marginEnd="30dp"
        android:text="금"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="279dp" />

</RelativeLayout>
